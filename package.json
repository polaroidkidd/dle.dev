{
  "name": "frontend",
  "version": "0.1.21",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --env.mode development --hot",
    "build:dev": "webpack --config ./utils/webpack/webpack.config.ts --env mode=development ",
    "build:prod": "webpack --config ./utils/webpack/webpack.config.ts --env mode=production  ",
    "docker:build": "utils/scripts/docker-build.sh",
    "docker:run": "docker run --rm -p 8080:80 eu.gcr.io/dle-dev/frontend:staging",
    "docker:run:dev": "run-s build:dev docker:build docker:run ",
    "docker:run:prod": "run-s build:prod docker:build docker:run",
    "build:staging": "yarn run build:prod --env depEnv=staging",
    "build:prod:analyze": "yarn run build:prod --env presets=analyze",
    "build:dev:analyze": "yarn run build:prod --env presets=analyze",
    "release": "standard-version",
    "bnd": "utils/scripts/build-and-deploy.sh",
    "lint": "eslint"
  },
  "homepage": "https://dle.dev",
  "husky": {
    "hooks": {
      "commit-msg": "./utils/scripts/vcs-check.sh",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "printWidth": 120,
    "tabWidth": 2,
    "bracketSpacing": true,
    "overrides": [
      {
        "files": ".prettierrc",
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.0-rc.2",
    "axios": "^0.20.0",
    "classnames": "^2.2.6",
    "csstype": "^3.0.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hot-loader": "^4.13.0",
    "react-loader-spinner": "^3.1.14",
    "react-markdown": "^4.3.1",
    "react-promise-tracker": "^2.1.0",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.2.1",
    "styled-components": "^5.2.0",
    "styled-icons": "^10.22.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/plugin-transform-unicode-regex": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.10",
    "@types/node": "^14.11.10",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-loader-spinner": "^3.1.1",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/styled-components": "^5.1.4",
    "@types/webfontloader": "^1.6.32",
    "@types/webpack": "^4.41.23",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "@next",
    "copy-webpack-plugin": "^6.2.1",
    "core-js": "^3.6.5",
    "css-loader": "^5.0.0",
    "file-loader": "^6.1.1",
    "html-webpack-plugin": "@next",
    "husky": "^4.3.0",
    "inline-source-map": "^0.6.2",
    "mini-css-extract-plugin": "^1.0.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "path-browserify": "^1.0.1",
    "postcss": "^8.1.1",
    "postcss-loader": "^4.0.4",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.1.0",
    "replace-ext": "^2.0.0",
    "sass-loader": "^10.0.3",
    "sass-resources-loader": "^2.1.1",
    "standard-version": "^9.0.0",
    "style-loader": "^2.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3",
    "typescript-plugin-css-modules": "^2.7.0",
    "unified": "^9.2.0",
    "url-loader": "^4.1.1",
    "vfile": "^4.2.0",
    "webpack": "^5.1.3",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0",
    "webpack-plugin-critical": "^1.0.0"
  }
}
