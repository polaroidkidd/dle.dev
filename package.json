{
  "name": "frontend",
  "version": "0.1.21",
  "private": true,
  "scripts": {
    "release": "standard-version",
    "webpack:build": "env-cmd -f .env.build webpack --config ./utils/webpack/webpack.config.ts",
    "webpack:serve": "env-cmd -f .env.build webpack serve --config ./utils/webpack/webpack.config.ts",
    "start": "yarn run webpack:serve --env mode=development --hot",
    "dev:build": "yarn run webpack:build --env mode=development ",
    "dev:build:analyze": "yarn run dev:build --env presets=analyze",
    "prod:build": "yarn run webpack:build --env mode=production  ",
    "prod:build:analyze": "yarn run prod:build --env presets=analyze",
    "docker:dev:run": "./utils/scripts/docker.sh -e dev -r",
    "docker:dev:build": "./utils/scripts/docker.sh -e dev",
    "docker:dev:release": "./utils/scripts/docker.sh -e dev -p",
    "docker:prod:build": "./utils/scripts/docker.sh -e prod",
    "docker:prod:run": "./utils/scripts/docker.sh -e prod -r",
    "docker:prod:release": "./utils/scripts/docker.sh -e prod -p"
  },
  "homepage": "https://dle.dev",
  "husky": {
    "hooks": {
      "commit-msg": "./utils/scripts/vcs-check.sh",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "printWidth": 120,
    "tabWidth": 2,
    "bracketSpacing": true,
    "overrides": [
      {
        "files": ".prettierrc",
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "dependencies": {
    "@hot-loader/react-dom": "^17.0.0-rc.2",
    "axios": "^0.20.0",
    "classnames": "^2.2.6",
    "csstype": "^3.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hot-loader": "^4.13.0",
    "react-loader-spinner": "^3.1.14",
    "react-markdown": "^4.3.1",
    "react-promise-tracker": "^2.1.0",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^15.2.1",
    "styled-components": "^5.2.0",
    "styled-icons": "^10.22.0",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/plugin-transform-object-assign": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/plugin-transform-unicode-regex": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.10",
    "@types/compression-webpack-plugin": "^4.0.1",
    "@types/html-webpack-plugin": "^3.2.4",
    "@types/mini-css-extract-plugin": "^1.0.0",
    "@types/node": "^14.14.2",
    "@types/react": "^16.9.53",
    "@types/react-dom": "^16.9.8",
    "@types/react-loader-spinner": "^3.1.1",
    "@types/react-syntax-highlighter": "^13.5.0",
    "@types/styled-components": "^5.1.4",
    "@types/webfontloader": "^1.6.32",
    "@types/webpack": "^4.41.23",
    "@types/webpack-dev-server": "^3.11.0",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^6.0.3",
    "copy-webpack-plugin": "^6.2.1",
    "core-js": "^3.6.5",
    "css-loader": "^5.0.0",
    "env-cmd": "^10.1.0",
