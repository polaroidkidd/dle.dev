{
  "name": "frontend",
  "version": "0.1.21",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --env.mode development --hot",
    "build:dev": "webpack --env.mode development",
    "build:prod": "webpack --env.mode production",
    "docker:build": "./docker-build.sh",
    "docker:run": "docker run --rm -p 8080:80 eu.gcr.io/dle-dev/frontend:staging",
    "docker:run:dev": "run-s build:dev docker:build docker:run ",
    "docker:run:prod": "run-s build:prod docker:build docker:run",
    "build:staging": "yarn run build:prod --env.depEnv staging",
    "build:prod:analyze": "yarn run build:prod --env.presets analyze",
    "build:dev:analyze": "yarn run  build:prod --env.presets analyze",
    "release": "standard-version",
    "bnd": "./build-and-deploy.sh",
    "lint": "eslint"
  },
  "homepage": "https://dle.dev",
  "husky": {
    "hooks": {
      "commit-msg": "./vcs-check.sh",
      "pre-commit": "pretty-quick --staged"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "IE 11"
  ],
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "printWidth": 120,
    "tabWidth": 2,
    "bracketSpacing": true,
    "overrides": [
      {
        "files": ".prettierrc",
        "options": {
          "parser": "json"
        }
      }
    ]
  },
  "dependencies": {
    "@hot-loader/react-dom": "^16.13.0",
    "axios": "^0.19.2",
    "classnames": "^2.2.6",
    "csstype": "^2.6.10",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-hot-loader": "^4.12.21",
    "react-loader-spinner": "^3.1.14",
    "react-markdown": "^4.3.1",
    "react-promise-tracker": "^2.1.0",
    "react-router-dom": "^5.2.0",
    "react-syntax-highlighter": "^12.2.1",
    "styled-components": "^5.1.0",
    "styled-icons": "^10.2.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-transform-object-assign": "^7.10.4",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/plugin-transform-unicode-regex": "^7.10.4",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/axios": "^0.14.0",
    "@types/classnames": "^2.2.10",
    "@types/node": "^14.0.1",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-loader-spinner": "^3.1.0",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/styled-components": "^5.1.0",
    "@types/webfontloader": "^1.6.29",
    "@types/webpack": "^4.41.13",
    "autoprefixer": "^10.0.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "clean-webpack-plugin": "^3.0.0",
    "compression-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^5.1.1",
    "core-js": "^3.6.5",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "inline-source-map": "^0.6.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.1.1",
    "postcss-loader": "^4.0.3",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "sass-loader": "^10.0.2",
    "sass-resources-loader": "^2.0.3",
    "standard-version": "^8.0.0",
    "style-loader": "^1.2.1",
    "ts-node": "^8.10.1",
    "typescript": "^3.9.2",
    "typescript-plugin-css-modules": "^2.3.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^4.2.2",
    "webpack-plugin-critical": "^1.0.0"
  }
}
